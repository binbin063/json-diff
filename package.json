  "name": "locale-json-diff",
  "description": "JSON diff for locale",
  "version": "1.0.1",
  "config": {
    "peridotpath": "/code/synchronoss/peridot/ux/src/components/"
  },
  "scripts": {
    "test:sync": "node bin/sync-json.js test/en.json test/ch.json",
    "test:find": "node bin/find-localeref.js test/en.json $npm_package_config_peridotpath",
    "test": "./node_modules/mocha/bin/mocha test/*.coffee",
    "cov": "rm -rf lib-cov; jscoverage lib lib-cov; env JSLIB=lib-cov mocha -R dot && env JSLIB=lib-cov mocha -R html-cov >coverage.html; open coverage.html"
  },
  "dependencies": {
    "cli-color": "~0.1.6",
    "difflib": "~0.2.1",
    "dreamopt": "~0.6.0"
  },
  "devDependencies": {
    "coffee-script": "^1.12.7",
    "mocha": "~1.7.0"
  },
  "optionalDependencies": {},
  "engines": {
    "node": "*"
  }
}
